E_ALERT     :: 0x07; // Alert
E_BACKSPACE :: 0x08; // Backspace
E_ESCAPE    :: 0x1B; // Escape
E_BREAK     :: 0x0C; // Page break
E_NEWLINE   :: 0x0A; // Newline
E_CARRIAGE  :: 0x0D; // Carriage return
E_TAB       :: 0x09; // Horizontal tab
E_V_TAB     :: 0x0B; // Vertical tab
E_BACKSLASH :: 0x5C; // Octal value
E_APOST     :: 0x27; // Apostrophe
E_QUOTE     :: 0x22; // Quotation mark