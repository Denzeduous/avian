Calculator :: bvr (?T: type) {
    add :: proc (x, y: T) -> T {
        return x + y;
    }

    sub :: proc (x, y: T) -> T {
        return x - y;
    }

    mul :: proc (x, y: T) -> T {
        return x * y;
    }

    div :: proc (x, y: T) -> T {
        return x / y;
    }

    mod :: proc (x, y: T) -> T {
        return x % y;
    }
}

main :: proc() {
    c :: Calculator(int);
    
    x := 10;
    y := 5;

    add := c.add(x, y); // 15
    sub := c.sub(x, y); // 10
    mul := c.mul(x, y); // 50
    div := c.div(x, y); // 10
    mod := c.mod(x, y); // 0
}