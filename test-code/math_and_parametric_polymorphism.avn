Calculator :: bvr (T: type) {
    add :: trait (x, y: T) -> T {
        return x + y;
    }

    sub :: trait (x, y: T) -> T {
        return x - y;
    }

    mul :: trait (x, y: T) -> T {
        return x * y;
    }

    div :: trait (x, y: T) -> T {
        return x / y;
    }

    mod :: trait (x, y: T) -> T {
        return x % y;
    }
}

main :: proc() {
    c := Calculator(int){};
    
    x := 10;
    y := 5;

    add := c.add(x, y);
    sub := c.sub(x, y);
    mul := c.mul(x, y);
    div := c.div(x, y);
    mod := c.mod(x, y);
}